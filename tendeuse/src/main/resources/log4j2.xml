<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN" name="tendeuse">
  <Appenders>
		<Console name="STDOUT" target="SYSTEM_OUT">
      		<PatternLayout pattern="%d %p %c{1.} [%t] %m%n"/>
    	</Console>
    </Appenders>
    <Loggers>
        <Root level="INFO">
            <AppenderRef ref="STDOUT"/>
        </Root>
    </Loggers>
</Configuration>